ad = "eyl"
sifre = 123
if (ad=="eyl" and sifre==123):
    print("giris basarilidir")
elif(ad=="eyl" and sifre!=123):
    print("sifre hatalidir")
else:
    (ad!="eyl" and sifre == 123)
    print("ad hatalidir")

