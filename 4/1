tercih_input =int(input("-HANGİ İSLEMİ TERCİH EDERSİNİZ?:3"))
sayi_input = int(input("ilk sayiyi giriniz:"))
sayi_input2 = int(input("ikinci sayiyi giriniz:"))


if tercih_input==1:
    print(sayi_input+sayi_input2)
elif tercih_input==2:
    print(sayi_input - sayi_input2)
elif tercih_input==3:
    print(sayi_input * sayi_input2)
else:
    tercih_input==4
    print(sayi_input/sayi_input2)
